package com.chandrika;

public class Boolean 
{
    public static void main(String[] args)
    {
        boolean b = true;
        //boolean b = 0;
        //boolean b = True;
        //boolean b = "True";

        System.out.println(b);

       // int x = 0;   // int can not converted to string
        boolean x = true;
        if (x)
        {
            System.out.println("Hello");
        }
        else
        {
            System.out.println("Hiii");
        }               
    }
}
